<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/bootstrap.extra.css")" rel="stylesheet" type="text/css" />
    
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/bootstrap-modal.js")" type="text/javascript"></script>
    <!-- jquery ui -->
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.min.js")" type="text/javascript"></script>
    
    
    @*<script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.5/jquery-ui.js" type="text/javascript"></script>*@

    <link href="http://ajax.microsoft.com/ajax/jquery.ui/1.8.5/themes/cupertino/jquery-ui.css" rel="stylesheet" type="text/css" />
    
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.tablesorter.min.js")" type="text/javascript"></script>
    <script  src="http://dev.jquery.com/view/trunk/plugins/calendar/jquery-calendar.js"></script>

    <script src="/Scripts/jquery.validate.js" type="text/javascript"></script>    <script src="/Scripts/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>    <script src="@Url.Content("~/Scripts/methods_es.js")" type="text/javascript"></script>

    <script>
        $(document).ready(function () {
            $(".datefield").datepicker();
            $("table.zebra-striped").tablesorter({ sortList: [[1, 0]] });
        });
    </script>
</head>
<body>


@*    
            Twitter bootstrap Modal Window Test
           <div id="my-modal" class="modal hide fade">
            <div class="modal-header">
              <a href="#" class="close">&times;</a>
              <h3>Modal Heading</h3>
            </div>
            <div class="modal-body">
              <p>One fine body…</p>
            </div>
            <div class="modal-footer">
              <a href="#" class="btn primary">Primary</a>
              <a href="#" class="btn secondary">Secondary</a>
            </div>
          </div>

    <a class="btn" data-controls-modal="my-modal" data-backdrop="static" >Launch Modal (example)</a>
*@      
    <div class="topbar">
        <div class="topbar-inner">
            <div class="container-fluid">
                <h3><a href="@Url.Action("Index", "Home")">ACME Invoicing App</a></h3>
                <p class="pull-right">
                    @Html.Partial("_LogOnPartial")
                </p>
            </div>
         </div>
    </div>

    <div class="container-fluid">
        <div class="sidebar">
            <h3>Admin</h3>
            @{Html.RenderPartial("Menu");}
        </div>
        <div class="content">
           @RenderBody()
        </div>
    </div>
</body>
</html>
